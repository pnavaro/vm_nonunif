  dimx   = 1.
  dimy   = 1.       	 	!dimensions du domaine 
  nx     = 40			!nombre de pts suivant x
  ny     = 40   		!nombre de pts suivant y
  cfl    = 0.8 			!nombre de Courant-Friedrich-Levy
  tfinal = 20.   		!temps final
  nstepmax  = 2000		!nbre d'iterations maxi
  nomcas = 'viry__'		!nom du cas traite (6 lettres)
  jname  = 'jneant'		!methode de calcul des courants
  icrea  = 1			!frequence d'emission des particules
  idiag  = 10    		!frequence des sorties graphiques
  bcname = "period"		!type de conditions limites
  exext  = 0.			!champ electrique exterieur suivant x
  eyext  = 1.	 		!champ electriaue exterieur suivant y
  bzext  = 4.			!champ magnetique exterieur
  charge = 1.d0         	!charge d'une macro particule
  masse  = 1.d0         	!masse d'une macro particule
  c      = 8.	 		!vitesse de la lumiere
  e0     = 1.                   !permittivite du vide
  relativ= .false.
$end
